{
  "filter_paths": "(dependencies/|lib/|mocks/|test/|script/|upgradedProtocol/)",
  "detectors_to_exclude": "conformance-to-solidity-naming-conventions,incorrect-versions-of-solidity,solc-version,timestamp",
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "disable_color": false,
  "legacy_ast": false,
  "exclude_dependencies": true,
  "solc_remaps": [
    "@openzeppelin/=lib/openzeppelin-contracts/",
    "@openzeppelin-53/=lib/openzeppelin-contracts-53/",
    "forge-std/=lib/forge-std/src/",
    "base64-sol/=lib/base64/",
    "@uniswap/lib/contracts/=lib/uniswap-lib/contracts/",
    "solmate/=lib/solmate/"
  ]
}